{
    "name": "Angular Template",
    "customizations": {
        "vscode": {
            "extensions": [
                "angular.ng-template",
                "dbaeumer.vscode-eslint"
            ]
        }
    },
    "updateContentCommand":"sudo apt update",
    "postCreateCommand": "sudo apt install mariadb-server -y",
    "portsAttributes": {
        "4200": {
            "label": "Angular Dev Server",
            "onAutoForward": "notify"
        }
    },
    "forwardPorts": [4200]
}
